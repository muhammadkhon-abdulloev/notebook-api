{
    "openapi": "3.0.0",
    "info": {
        "title": "Notebook API",
        "contact": {
            "email": "admin@sartoj.tech"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api/v1",
            "description": "Notebook API server"
        }
    ],
    "paths": {
        "/notebook": {
            "get": {
                "tags": [
                    "Notebook"
                ],
                "summary": "Display all Notebooks. 10 items per page",
                "operationId": "notebookAll",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "The page number.",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "4"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Everything is fine"
                    }
                }
            },
            "post": {
                "tags": [
                    "Notebook"
                ],
                "summary": "Create yet another notebook",
                "operationId": "notebookCreate",
                "requestBody": {
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Everything is fine"
                    }
                }
            }
        },
        "/notebook/{id}": {
            "get": {
                "tags": [
                    "Notebook"
                ],
                "summary": "Display Notebook by ID",
                "operationId": "notebookByID",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The id of the Notebook",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "1"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Everything is fine"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Notebook"
                ],
                "summary": "Delete Notebook by ID",
                "operationId": "deleteByID",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The id of the Notebook",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "5"
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Note deleted"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Notebook",
            "description": "To get notebooks from database"
        }
    ]
}